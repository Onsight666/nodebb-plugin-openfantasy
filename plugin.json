{
	"id": "nodebb-plugin-openfantasy",
	"name": "OpenFantasy",
	"description": "A fully featured RPG (Role Playing Game) plugin for NodeBB",
	"url": "https://github.com/psychobunny/nodebb-plugin-openfantasy",
	"library": "./openfantasy.js",
	"hooks": [
		{
			"hook": "filter:header.build", "method": "addNavigation"
		},
		{
			"hook": "filter:server.create_routes", "method": "addRoute"
		},
		{
			"hook": "action:app.load", "method": "init"
		}
	],
	"staticDir": "./static",
	"templates": "./templates",
	"css": [
		"style.css"
	]
}